{
  "manifest_version": 2,
  "name": "Tabulator",
  "version": "0.7.0",
  "description": "Like OneTab but leaner and FOSS",
  "icons": {},
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Tabulator"
  },
  "background": {
    "page": "tabulator.html",
    "persistent": false
  },
  "commands": {
    "toggle-jump-index": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Toggle jump index on tabs."
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "modal.js",
        "js/jquery.js"
      ]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "contextMenus"
  ]
}
